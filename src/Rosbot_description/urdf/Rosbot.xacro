<link name="lidar_1">
  <inertial>
    <origin xyz="0.0 0.0 0.009999999999999981" rpy="0 0 0"/>
    <mass value="0.0754575997873079"/>
    <inertia ixx="1.9e-05" iyy="1.9e-05" izz="3.3e-05" ixy="0.0" iyz="0.0" ixz="0.0"/>
