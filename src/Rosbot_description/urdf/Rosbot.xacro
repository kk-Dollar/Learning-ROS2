    <inertia ixx="0.008048" iyy="0.008737" izz="0.014943" ixy="-0.0" iyz="0.0" ixz="-0.000482"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
